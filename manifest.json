{
  "manifest_version": 2,

  "name": "Inject script and chart",
  "description": "This extension will eject new script tag into dom",
  "version": "1.0",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["lib/jquery.js", "js/load/chart.js", "js/load/popover.js"]
    }
  ],
  "web_accessible_resources": [
    "img/*.png",
    "js/**/*",
    "html/*",
    "css/*",
    "lib/jquery.js"
  ]
}
